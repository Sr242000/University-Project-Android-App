<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SportsSmallLetterMenu2"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="189dp">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="414dp"
        android:layout_height="67dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/small_letter_sports_text"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="Small Letters"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sports_text_select_the_letter"
        android:layout_width="232dp"
        android:layout_height="46dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="Select the Letter"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3" />

    <Button
        android:id="@+id/sports_small_v_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="61dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="v"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_next_button"
        app:layout_constraintEnd_toStartOf="@+id/sports_small_w_button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_x_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="61dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="x"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_previous_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sports_small_w_button" />

    <Button
        android:id="@+id/sports_small_w_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="61dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="w"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_next_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_s_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="41dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="s"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_v_button"
        app:layout_constraintEnd_toStartOf="@+id/sports_small_t_button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_t_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="41dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="t"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_w_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_u_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="41dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="u"
        style="@style/CustomButtonStyle"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_x_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sports_small_t_button"
        android:textSize="20sp" />

    <Button
        android:id="@+id/sports_small_p_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="p"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_s_button"
        app:layout_constraintEnd_toStartOf="@+id/sports_small_q_button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_q_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="q"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_t_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_r_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="r"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_u_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sports_small_q_button" />

    <Button
        android:id="@+id/sports_small_m_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="43dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="m"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_p_button"
        app:layout_constraintEnd_toStartOf="@+id/sports_small_n_button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_o_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="43dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="o"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_r_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sports_small_n_button" />

    <Button
        android:id="@+id/sports_small_n_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="43dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="n"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sports_small_q_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_next_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="125dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="Next"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/sports_small_previous_button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/sports_small_previous_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="124dp"
        android:fontFamily="@font/helvetica_now_text_medium"
        android:text="Previous"
        style="@style/CustomButtonStyle"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sports_small_next_button" />

</androidx.constraintlayout.widget.ConstraintLayout>