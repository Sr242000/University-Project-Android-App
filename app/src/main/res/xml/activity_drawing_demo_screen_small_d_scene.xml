            android:id="@+id/colour_palette_demo_small_d" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/path_5"
        motion:constraintSetEnd="@+id/path_6"
        motion:autoTransition="animateToEnd"
        motion:pathMotionArc="startVertical"
        motion:duration="1000" />
    <ConstraintSet
        android:id="@+id/path_7"
        motion:deriveConstraintsFrom="@+id/path_6" >
        <Constraint
            android:layout_height="84dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="204dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.696"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="98dp"
            android:id="@+id/colour_palette_demo_small_d" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/path_6"
        motion:constraintSetEnd="@+id/path_7"
        motion:autoTransition="animateToEnd"
        motion:pathMotionArc="startHorizontal"
        motion:duration="1000" />
</MotionScene>