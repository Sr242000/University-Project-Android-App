            android:id="@+id/cookie_demo_small_t" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/path_3"
        motion:constraintSetEnd="@+id/path_4"
        motion:autoTransition="animateToEnd"
        motion:duration="1000" />
    <ConstraintSet
        android:id="@+id/path_5"
